<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="description" content="">
  <meta name="author" content="">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>PhotoCywoniuk</title>
  <%= stylesheet_link_tag    "admin", :media => "all" %>
  <%= javascript_include_tag "admin" %>
  <%= csrf_meta_tags %>
  <%= yield :head %>
  <%= yield :location_libraries %>
</head>
<body>
  <header>
    <section>
      <%= yield :header %>
    </section>
  </header>
  
  <section id="wrapper">
    <section class="main">
      <section class="flash">
        <% flash.each do |key, msg| %>
          <div id="<%= key %>">
            <% if msg.kind_of?(Array) %>
              <% msg.each do |m| %>
                <p><%= m %></p>
              <% end %>
            <% else %>
              <p><%= msg %></p>
            <% end %>
          </div>
        <% end %>
      </section>
      <%= yield %>
    </section>
  </section>
</body>
</html>
